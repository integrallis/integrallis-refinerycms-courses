<%= form_for [:admin, @course_event_instructor] do |f| -%>
  <%= render :partial => "/shared/admin/error_messages", :locals => {
          :object => @course_event_instructor,
          :include_object_name => true
        } %>
  
  <div class='field'>
    <%= f.label :course_event_id -%>
    <%= f.text_field :course_event_id, :class => 'larger widest' -%>
  </div>
  
  <div class='field'>
    <%= f.label :instructor_id -%>
    <%= f.text_field :instructor_id -%>
  </div>
  
  <%= render :partial => "/shared/admin/form_actions",
              :locals => {
                :f => f,
                :continue_editing => false,
                :delete_title => t('delete', :scope => 'admin.course_event_instructors.course_event_instructor'),
                :delete_confirmation => t('message', :scope => 'shared.admin.delete')
              } %>
<% end -%>
